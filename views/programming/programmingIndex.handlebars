<header class="index-header">
编程
<span><a href="/">我的列表</a></span>
</header>  
<div class="container mt3">
   <div class="row">
      <!--左边-->
      <div class="col-md-4">
        <div class="list-group">
          <a href="#" class="list-group-item active">单页web应用（SinglePageWebapplication）</a>
          <a href="#" class="list-group-item">上传</a>
          <a href="#" class="list-group-item">教程</a>
        </div>
      </div>
      <!--右边-->
      <div class="col-md-8">
        <script id="p" type="text/x-handlebars">
          <p>This is a partial.</p>
          <p>{{errors}}</p>
        </script>
        <script id="t" type="text/x-handlebars">
          <p>This is the template.</p>
           {{#if errors}}
            {{> error}}
            {{/if}}    
        </script>
        <div></div><div></div>
      </div>      
   </div>
</div>
<script> 
Handlebars.registerPartial('error', $('#p').html());
var t = Handlebars.compile($('#t').html());
$('div:first').html(t({ errors: 'It done broke' }));
$('div:last').html(t({ errors: null }));
</script>